{
    "compilerOptions": {
      "outDir": "./dist/",
      "noImplicitAny": true,
      "sourceMap": true,
      "module": "es6",
      "target": "es5",
      "jsx": "react",
      "allowJs": true,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,

      "lib": ["es6", "dom"],
      "noImplicitThis": true,
      "strictNullChecks": true,
      // "strict": true,
    }
  }